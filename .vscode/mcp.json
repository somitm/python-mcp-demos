{
	"servers": {
		"expenses-mcp": {
			"type": "stdio",
			"command": "uv",
			"cwd": "${workspaceFolder}",
			"args": [
				"run",
				"servers/basic_mcp_stdio.py"
			]
		},
		"expenses-mcp-http": {
			"type": "http",
			"url": "http://localhost:8000/mcp"
		},
		"expenses-mcp-debug": {
			"type": "stdio",
			"command": "uv",
			"cwd": "${workspaceFolder}",
			"args": [
				"run",
				"--",
				"python",
				"-m",
				"debugpy",
				"--listen",
				"0.0.0.0:5678",
				"servers/basic_mcp_stdio.py"
			]
		},
		"auth-expenses": {
			"url": "https://mcp-gps-key-n7pc5ej-ca.ashymeadow-ae27942e.eastus2.azurecontainerapps.io/mcp",
			"type": "http"
		}
	},
	"inputs": []
}