{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "name": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "useMonitoring": {
        "value": "${USE_MONITORING=true}"
      },
      "useVnet": {
        "value": "${USE_VNET=false}"
      },
      "usePrivateIngress": {
        "value": "${USE_PRIVATE_INGRESS=false}"
      },
      "acaExists": {
        "value": "${SERVICE_ACA_RESOURCE_EXISTS=false}"
      },
      "mcpnoauthExists": {
        "value": "${SERVICE_MCPNOAUTH_RESOURCE_EXISTS=false}"
      },
      "keycloakExists": {
        "value": "${SERVICE_KEYCLOAK_RESOURCE_EXISTS=false}"
      },
      "keycloakAdminUser": {
        "value": "${KEYCLOAK_ADMIN_USER=admin}"
      },
      "keycloakAdminPassword": {
        "value": "${KEYCLOAK_ADMIN_PASSWORD}"
      },
      "keycloakRealmName": {
        "value": "${KEYCLOAK_REALM_NAME=mcp}"
      },
      "mcpServerAudience": {
        "value": "${MCP_SERVER_AUDIENCE=mcp-server}"
      }
    }
  }